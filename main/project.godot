config/icon="res://Assets/ParasiteEye.png"
config/image="res://Assets/Parasite(big).png"
config/size/viewport_height=576

[autoload]

DamageNumbersServer="*res://Systems/damage_numbers_server.gd"
LevelTransition="*res://Transitions/level_transition.tscn"
BetterTerrain="*res://addons/better-terrain/BetterTerrain.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1

[display]

window/size/viewport_width=1024
window/size/viewport_height=576
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Parasite"

[editor_plugins]

enabled=PackedStringArray("res://addons/better-terrain/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg")

[gui]

theme/custom="res://Theme/DefaultTheme.tres"
